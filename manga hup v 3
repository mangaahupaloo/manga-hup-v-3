local ScreenGui = Instance.new("ScreenGui")
local Frame = Instance.new("Frame")
local ContinueButton = Instance.new("TextButton")

ScreenGui.Name = "MangaHub"
ScreenGui.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")

Frame.Name = "MainFrame"
Frame.Parent = ScreenGui
Frame.BackgroundColor3 = Color3.new(0.1, 0.1, 0.1)
Frame.Position = UDim2.new(0.5, -100, 0.5, -50)
Frame.Size = UDim2.new(0, 200, 0, 100)
Frame.AnchorPoint = Vector2.new(0.5, 0.5)

ContinueButton.Name = "ContinueButton"
ContinueButton.Parent = Frame
ContinueButton.Text = "Continue"
ContinueButton.Size = UDim2.new(0, 180, 0, 50)
ContinueButton.Position = UDim2.new(0.5, -90, 0.5, -25)
ContinueButton.BackgroundColor3 = Color3.new(0.2, 0.6, 0.2)
ContinueButton.TextColor3 = Color3.new(1, 1, 1)
ContinueButton.Font = Enum.Font.SourceSansBold
ContinueButton.TextSize = 18

local function collectMoneyBoxes()
    for _, box in pairs(game.Workspace:GetDescendants()) do
        if box:IsA("Part") and box.Name == "Chest" then
            local player = game.Players.LocalPlayer
            local character = player.Character or player.CharacterAdded:Wait()
            local humanoidRootPart = character:WaitForChild("HumanoidRootPart")

            if (box.Position - humanoidRootPart.Position).Magnitude <= 300 then
                humanoidRootPart.CFrame = box.CFrame
                wait(0.5)
            end
        end
    end
end

ContinueButton.MouseButton1Click:Connect(function()
    collectMoneyBoxes()
end)
